{
  "manifest_version": 3,
  "name": "BibTeX Extractor",
  "version": "1.0",
  "description": "Extracts BibTeX from DOIs found on any webpage.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}

  